                    android:src="@drawable/m21"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:id="@+id/m21"
                    android:background="@drawable/borderimg"
                    android:cropToPadding="true"
                    android:scaleType="fitXY"/>
